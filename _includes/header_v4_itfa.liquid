<header>
  <!-- Nav Bar -->
  <nav id="navbar" class="navbar navbar-light navbar-expand-sm {% if site.navbar_fixed %}fixed-top{% else %}sticky-top{% endif %}" role="navigation">
    <div class="container">
      {% if page.permalink != '/' %}
        <a class="navbar-brand title font-weight-lighter" href="{{ site.baseurl }}/itfasubmission">
          {% if site.title == 'blank' %}
            {% if site.first_name %}
              <span class="font-weight-bold">
                {{- site.first_name -}}
              </span>
            {% endif %}
            {% if site.middle_name %}
              {{- site.middle_name -}}
            {% endif %}
            {% if site.last_name %}
              {{- site.last_name -}}
            {% endif %}
          {% else %}
            Demistifying AI
          {% endif %}
        </a>
      {% endif %}
      </div>
  </nav>
</header>
